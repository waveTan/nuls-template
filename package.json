    "build": "cross-env NULS_ENV=prod vue-cli-service build",
    "build-beta": "cross-env NULS_ENV=beta vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-to-clipboard": "^3.1.0",
    "cross-env": "^6.0.3",
    "element-ui": "^2.4.5",
    "nuls-sdk-js": "^2.2.7",
    "shelljs": "^0.8.3",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
